<div class="form-card">
  <%= form_with(model: order, html: { multipart: true }) do |f| %>

    

    <div class="form-group">
      <%= f.label :part_number %>
      <%= f.text_field :part_number %>
    </div>


    <div class="form-group">
      <%= f.label :price %>
      <%= f.number_field :price %>
    </div>

    <%= f.submit %>

  <% end %>
</div>
